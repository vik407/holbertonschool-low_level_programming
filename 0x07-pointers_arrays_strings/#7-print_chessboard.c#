#include "holberton.h"

/**
 * print_diagsums - print the sums of the diagonals of a                                                                                                                          
 * square matrix of integers                                                                                                                                                      
 * @a: matrix                                                                                                                                                                     
 * @size: size each side of matrix.
 *
 * @a: pointer array to chess characters
 * Return: a game.
 **/

void print_chessboard(char (*a)[8])
{
	int i, j;

	for (i = 0; i < 8; i++)
	{
		for (j = 0; j < 8; j++)
		{
			_putchar(a[i][j]);
		}
		_putchar('\n');
	}
}
